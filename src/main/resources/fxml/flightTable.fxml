            <TableColumn fx:id="originColumn" prefWidth="88.0" text="origin" />
            <TableColumn fx:id="destColumn" prefWidth="115.0" text="destination" />
            <TableColumn fx:id="distanceColumn" prefWidth="86.0" text="distance" />
